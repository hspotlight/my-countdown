{
    "version": 2,
    "env": {
    },
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["backend/build/**"] }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/build/index.js"
      },
      {
        "src": "/",
        "dest": "ui/build/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "ui/build/$1"
      }
    ]
  }